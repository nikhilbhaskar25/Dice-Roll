   "source": [
    "import random\n",
    "\n",
    "def roll_dice(num_dice):\n",
    "    rolls = []\n",
    "    for _ in range(num_dice):\n",
    "     rolls.append(random.randint(1, 6))\n",
    "    return rolls\n",
    "\n",
    "num_dice = int(input(\"How many dice do you want to roll? \"))\n",
    "results = roll_dice(num_dice)\n",
    "\n",
    "print(\"You rolled these numbers:\", results)"
   ]
